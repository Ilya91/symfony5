{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": ">=7.2.5",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "api-platform/core": "2.*",
        "composer/package-versions-deprecated": "*",
        "doctrine/annotations": "1.*",
        "doctrine/doctrine-bundle": "*",
        "doctrine/doctrine-migrations-bundle": "*",
        "doctrine/orm": "*",
        "elasticsearch/elasticsearch": "*",
        "fzaninotto/faker": "*",
        "nelmio/cors-bundle": "2.*",
        "nesbot/carbon": "2.*",
        "phpdocumentor/reflection-docblock": "5.*",
        "sensio/framework-extra-bundle": "5.*",
        "symfony/asset": "5.*",
        "symfony/console": "5.*",
        "symfony/dotenv": "5.*",
        "symfony/expression-language": "5.*",
        "symfony/flex": "*",
        "symfony/form": "5.*",
        "symfony/framework-bundle": "5.*",
        "symfony/http-client": "5.*",
        "symfony/intl": "5.*",
        "symfony/mailer": "5.*",
        "symfony/mime": "5.*",
        "symfony/monolog-bundle": "*",
        "symfony/notifier": "5.*",
        "symfony/process": "5.*",
        "symfony/property-access": "5.*",
        "symfony/property-info": "5.*",
        "symfony/security-bundle": "5.*",
        "symfony/serializer": "5.*",
        "symfony/string": "5.*",
        "symfony/translation": "5.*",
        "symfony/twig-bundle": "5.*",
        "symfony/validator": "5.*",
        "symfony/web-link": "5.*",
        "symfony/webpack-encore-bundle": "1.*",
        "symfony/yaml": "5.*",
        "symfonycasts/verify-email-bundle": "*",
        "twig/extra-bundle": "*",
        "twig/twig": "*"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "*",
        "lctrs/psalm-psr-container-plugin": "^1.1",
        "overtrue/phplint": "^2.3",
        "psalm/plugin-phpunit": "^0.15.0",
        "squizlabs/php_codesniffer": "3.*",
        "symfony/browser-kit": "5.*",
        "symfony/css-selector": "5.*",
        "symfony/debug-bundle": "5.*",
        "symfony/maker-bundle": "*",
        "symfony/phpunit-bridge": "5.*",
        "symfony/stopwatch": "5.*",
        "symfony/var-dumper": "5.*",
        "symfony/web-profiler-bundle": "5.*",
        "vimeo/psalm": "^4.3"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "lint": "phplint",
        "phpcs": "phpcs --standard=phpcs.xml",
        "phpcbf": "phpcbf --standard=phpcs.xml",
        "psalm": "psalm --config=psalm.xml",
        "test": "phpunit --colors=always",
        "test-coverage": "XDEBUG_MODE=coverage phpunit --colors=always --coverage-html var/test/coverage",
        "app": "php bin/app.php --ansi"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "5.*"
        }
    }
}
